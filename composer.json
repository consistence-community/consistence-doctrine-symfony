{
	"name": "consistence-community/consistence-doctrine-symfony",
	"description": "Symfony Bundle integrating Consistence library with Doctrine ORM",
	"keywords": ["consistence", "doctrine", "orm", "symfony", "bundle"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.4 || ~8.0",
		"consistence-community/consistence-doctrine": "~2.1",
		"doctrine/doctrine-bundle": "~2.2",
		"symfony/config": "~4.4|~5.2",
		"symfony/dependency-injection": "~4.4|~5.2",
		"symfony/http-kernel": "~4.4|~5.2",
		"symfony/yaml": "~4.4|~5.2"
	},
	"require-dev": {
		"consistence-community/coding-standard": "3.11.0",
		"php-parallel-lint/php-parallel-lint": "1.2.0",
		"phing/phing": "2.16.4",
		"phpunit/phpunit": "9.5.2"
	},
	"replace": {
		"consistence/consistence-doctrine-symfony": "2.*"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\Doctrine\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\Doctrine\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
